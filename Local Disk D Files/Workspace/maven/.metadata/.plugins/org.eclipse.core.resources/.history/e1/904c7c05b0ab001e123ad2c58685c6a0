package com.zettamine.mt;

public class Account {
	private Integer bal;

	public Integer getbal() {
		return bal;
	}

	public void setInitialDeposit(Integer initialDeposit) {
		this.bal = bal;
	}

	public Account(Integer initialDeposit) {
		super();
		this.bal = bal;
	}

	public void deposit(Integer amount) {
		this.bal+=amount;
		System.out.println(bal);
	}
	
	public void withdraw(Integer amount) {
		this.bal-=amount;
	}
	
}
